{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "upgrade": {
    "source": "cdn",
    "interval": "168h",
    "auto": false,
    "notice": false
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": " âš¡",
          "foreground": "#dd1e1e",
          "background": "#000000",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue62a",
            "wsl": "\ue712"
          },
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "foreground": "#ffffff",
          "background": "#000000",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "folder_icon": "\ue5fe",
            "folder_separator_icon": " <#000000>\ue0bd </>",
            "home_icon": "\ueb06",
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "foreground": "#43CCEA",
          "background": "#272727",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "template": "<#000000>\ue0b1 </>{{ .HEAD }}{{ if .Staging.Changed }}<#FF6F00> \uf046 {{ .Staging.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#00ff0d",
          "background": "#272727",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "style": "dallas",
            "threshold": 0
          },
          "trailing_diamond": "\ue0b0",
          "template": "<#000000>\ue0b1 </>{{ .FormattedMs }}s ",
          "foreground": "#ffffff",
          "background": "#272727",
          "type": "executiontime",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "leading_diamond": "\ue0c5",
          "template": " {{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}<transparent> / </>{{ .HostName }}",
          "foreground": "#43CCEA",
          "background": "#272727",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "precision": 2
          },
          "template": " \ue266 {{ round .PhysicalPercentUsed .Precision }}% ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#8f43f3",
          "type": "sysinfo",
          "style": "powerline"
        },
        {
          "foreground": "#00be0afa",
          "powerline_symbol": "\ue0b0",
          "background": "#8822ee",
          "type": "connection",
          "style": "powerline"
        },
        {
          "properties": {
            "time_format": "2006-01-02 15:04:05 -07:00"
          },
          "trailing_diamond": "\ue0c5",
          "template": "<#000000> \ue31e\uf0fb </>{{ .CurrentDate | date .Format }} ",
          "foreground": "#43CCEA",
          "background": "#272727",
          "type": "time",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true
          },
          "template": "\uf432 ",
          "foreground": "#00ff0d",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0000{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
